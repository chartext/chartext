{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "chartext",
  "version": "0.0.1",
  "description": "React Skia Chart",
  "keywords": [
    "canvaskit",
    "chart",
    "react",
    "skia"
  ],
  "bugs": "https://github.com/chartext/chartext/issues",
  "license": "MIT",
  "author": {
    "name": "threaddot",
    "email": "threaddot@users.noreply.github.com",
    "url": "https://github.com/threaddot"
  },
  "packageManager": "pnpm@8.5.0",
  "repository": "chartext/chartext.git",
  "scripts": {
    "canvaskit": "pnpm --filter @chartext/canvaskit",
    "chart": "pnpm --filter @chartext/chart",
    "clean": "pnpm -r clean",
    "dist": "pnpm -r dist",
    "examples:chart": "pnpm --filter @examples/chart",
    "lint:check": "eslint --cache --ext .ts,.tsx --ignore-path .eslintignore --config .eslintrc.yml examples/* packages/*",
    "lint:fix": "pnpm lint --fix",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "prettier:check": "prettier --debug-check --ignore-unknown **/*",
    "prettier:fix": "prettier --write --ignore-unknown **/*",
    "syncpack": "syncpack",
    "syncpack:fix-mismatches": "syncpack fix-mismatches",
    "syncpack:format": "syncpack format",
    "syncpack:list-mismatches": "syncpack list-mismatches",
    "test": "pnpm -r test",
    "tsc": "pnpm -r tsc"
  },
  "devDependencies": {
    "@alloc/fast-rimraf": "^1.0.8",
    "@testing-library/react": "^14.0.0",
    "@tsconfig/vite-react": "^2.0.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitejs/plugin-react-swc": "^3.3.1",
    "@webgpu/types": "^0.1.32",
    "dts-bundle-generator": "^8.0.1",
    "eslint": "^8.40.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^9.19.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "syncpack": "^9.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-progress": "^0.0.7",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.0"
  },
  "private": true
}
