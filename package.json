{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "chartext",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.3.1",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dist": "pnpm -r dist",
    "test:run": "pnpm -r test:run",
    "canvaskit:tsc": "pnpm --filter @chartext/canvaskit tsc",
    "canvaskit:dist": "pnpm --filter @chartext/canvaskit dist",
    "canvaskit:test": "pnpm --filter @chartext/canvaskit test:run",
    "chart:dist": "pnpm --filter @chartext/chart dist",
    "chart:test:run": "pnpm --filter @chartext/chart-tests test:run",
    "chart:test:watch": "pnpm --filter @chartext/chart test:watch",
    "examples:chart": "pnpm --filter @chartext/examples-chart dev",
    "syncpack:fix-mismatches": "syncpack fix-mismatches",
    "syncpack:format": "syncpack format",
    "syncpack:list": "syncpack list"
  },
  "devDependencies": {
    "@alloc/fast-rimraf": "^1.0.8",
    "@testing-library/react": "^14.0.0",
    "@tsconfig/vite-react": "^1.0.1",
    "@types/babel__core": "^7.20.0",
    "@types/react": "^18.0.32",
    "@types/react-dom": "^18.0.11",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^3.1.0",
    "@webgpu/types": "^0.1.30",
    "dts-bundle-generator": "^8.0.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^9.1.0",
    "husky": "^8.0.3",
    "prettier": "2.8.7",
    "syncpack": "^9.8.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.2.0",
    "vite-plugin-progress": "^0.0.7",
    "vite-tsconfig-paths": "^4.0.8",
    "vitest": "^0.29.8"
  }
}
